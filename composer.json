{
  "name": "strifejeyz/framework",
  "authors": [
    {
      "name": "Jesse Strife"
    }
  ],
  "description": "A Fast and Lightweight PHP MVC Framework.",
  "keywords": [
    "framework",
    "strife",
    "MVC",
    "RAD",
    "OOP"
  ],
  "license": "MIT",
  "version": "1.5",
  "type": "project",
  "minimum-stability": "stable",
  "require": {
    "php": ">= 8.1",
    "phpmailer/phpmailer": "^6.3",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      "kernel",
      "kernel/database",
      "kernel/security",
      "app/models"
    ],
    "files": [
      "app/config/application.php",
      "kernel/helpers.php",
      "app/routes.php",
      "app/config/database.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
